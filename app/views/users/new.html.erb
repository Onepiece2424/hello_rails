<h1>ユーザー新規登録</h1>

<table>
  <thead>
    <%= form_with model: @user do |f| %>
    <tr>
      <th>アドレス</th>
      <th><%= f.text_field :email %></th>
    </tr>
    <tr>
      <th>名前</th>
      <th><%= f.text_field :name %></th>
    </tr>
    <tr>
      <th></th>
      <th><br><%= f.submit "ユーザー登録" %></th>
    </tr>
    <% end %>
   </thead>
</table>
<br>
<br>
<%= link_to "ユーザー一覧へ", users_path %>
